{
  "name": "Apple Music Bot",
  "description": "A powerful Telegram music bot that can play music in groups via voice chat.",
  "repository": "https://github.com/GOVIND-BOTS/AppleMusic",
  "logo": "https://telegra.ph/file/your-logo.png",
  "keywords": ["telegram", "music", "bot", "pyrogram", "python"],
  "env": {
    "API_ID": {
      "description": "Your Telegram API ID from my.telegram.org",
      "required": true
    },
    "API_HASH": {
      "description": "Your Telegram API HASH from my.telegram.org",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "Your bot token from BotFather",
      "required": true
    },
    "SESSION_STRING": {
      "description": "Pyrogram session string for userbot",
      "required": true
    }
  },
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ],
  "addons": [],
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "free"
    }
  },
  "stack": "heroku-20"
}
