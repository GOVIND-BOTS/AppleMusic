{
  "name": "AppleMusicBot",
  "description": "A Telegram Music Bot with Apple Music and YouTube support.",
  "repository": "https://github.com/GOVIND-BOTS/AppleMusic",
  "keywords": ["telegram", "music", "bot", "apple-music", "youtube"],
  "env": {
    "API_ID": {
      "description": "Your Telegram API ID from my.telegram.org",
      "required": true
    },
    "API_HASH": {
      "description": "Your Telegram API Hash from my.telegram.org",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "Your Telegram Bot Token from @BotFather",
      "required": true
    },
    "SESSION_STRING": {
      "description": "Your Pyrogram session string",
      "required": true
    }
  },
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ],
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "free"
    }
  },
  "addons": [],
  "stack": "heroku-20"
}
